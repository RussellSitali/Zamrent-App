{
  "expo": {
    "name": "zamrent",
    "slug": "zamrent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/zamrentreal.png",
    "scheme": "zamrent",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/zamrent33.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.zamrent.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/zamrent33.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/zamrent33.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "linking": {
          "prefixes": ["zamrent://"],
          "config": {
            "screens": {
              "passwordscreen": "passwordscreen",
              "verifyemail": "verifyEmailScreen"
            }
          }
        }
      },
      "eas": {
        "projectId": "7f312cf1-3a74-4ab5-9c61-f894852eeec1"
      }
    }
  }
}
